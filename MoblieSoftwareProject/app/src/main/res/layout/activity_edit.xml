<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textAlignment="center"
    tools:context=".EditActivity">

    <LinearLayout
        android:id="@+id/linearLayout13"
        android:layout_width="422dp"
        android:layout_height="379dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout11">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginTop="2dp"
            app:srcCompat="@drawable/review" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="23dp"
            android:layout_height="344dp"
            android:layout_marginTop="6dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="@drawable/edge"
            android:fontFamily="@font/tmonmonsori"
            android:paddingTop="5dp"
            android:text="  음식 소감 또는 평을 입력하세요"
            android:textSize="15dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="421dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout10"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginTop="2dp"
            app:srcCompat="@drawable/place" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="6dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="@drawable/edge"
            android:fontFamily="@font/tmonmonsori"
            android:paddingTop="5dp"
            android:text="  식사 장소를 선택하세요"
            android:textSize="15dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="421dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout9"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginTop="2dp"
            app:srcCompat="@drawable/image" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="6dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="@drawable/edge"
            android:fontFamily="@font/tmonmonsori"
            android:paddingTop="5dp"
            android:text="  식사 사진을 업로드하세요"
            android:textSize="15dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="421dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginTop="2dp"
            app:srcCompat="@drawable/kcal" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="6dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="@drawable/edge"
            android:fontFamily="@font/tmonmonsori"
            android:paddingTop="5dp"
            android:text="  음식 칼로리를 입력하세요"
            android:textSize="15dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="419dp"
        android:layout_height="65dp"
        android:background="#E3EEB3"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="173dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/logo" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="419dp"
        android:layout_height="40dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <TextView
            android:id="@+id/Curr_date"
            android:layout_width="165dp"
            android:layout_height="33dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/tmonmonsori"
            android:textColor="@color/black" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="421dp"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5"
        tools:layout_editor_absoluteX="-1dp">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginTop="2dp"
            app:srcCompat="@drawable/type" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="6dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="@drawable/edge"
            android:fontFamily="@font/tmonmonsori"
            android:paddingTop="5dp"
            android:text="  식사 유형을 선택해주세요"
            android:textSize="15dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="421dp"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginTop="2dp"
            app:srcCompat="@drawable/time" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="6dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="@drawable/edge"
            android:fontFamily="@font/tmonmonsori"
            android:paddingTop="5dp"
            android:text="  식사 시간을 입력하세요"
            android:textSize="15dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="421dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginTop="2dp"
            app:srcCompat="@drawable/name" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="6dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="@drawable/edge"
            android:fontFamily="@font/tmonmonsori"
            android:paddingTop="5dp"
            android:text="  음식 이름 및 수량을 입력하세요"
            android:textSize="15dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="85dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout13">

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="90dp"
            android:layout_height="26dp"
            android:padding="0sp"
            android:scaleType="centerCrop"
            android:layout_marginTop="10dp"
            android:layout_gravity="center"
            android:src="@drawable/button4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout13"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/savebutton" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>